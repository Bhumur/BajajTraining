package com.example.demo.pecs;

import java.lang.reflect.Method;
import java.util.*;

// Producer Extends Consumer Super
public class PecsDemo {
	
	public static void prodMethod(List<? extends Number> list) {
		
	}
	
	public static void prodMethod(List<? super Number> list) {
		
	}
	
	public static void main(String []args) {
		List<Number> numberList = Arrays.asList(10,20,30,40);
		
		List<Integer> integerList = Arrays.asList(888,999,777,666);
		
		List<Double> doubleList = Arrays.asList(111.00,222.00,333.00);
		
		
		//Point one
		method(numberList);
		
	}

	private static void method(List<Number> numberList) {
		numberList.forEach(System.out::println);
		
	}

}
