package com.example.demo;

import java.util.ArrayList;
import java.util.Arrays;
import java.util.List;
import java.util.stream.Collectors;

public class FlatMapAndPartitionBy {

	public static void main(String[] args) {

		List<Integer> marks1 = Arrays.asList(90, 80, 70, 60);
		List<Integer> marks2 = Arrays.asList(50, 40, 30, 20);
		
		List<List<Integer>> combineList = new ArrayList<>();
		
		combineList.add(marks1);
		combineList.add(marks2);
		
		List<Integer> mappedList = combineList.stream()
									.flatMap(e -> e.stream())
									.collect(Collectors.toList());
									

	}

}
