package com.exercise.automobiles;

import com.exercise.iface.Automobile;

public class ShowRoom {
	Automobile vehicle;
	
	public MarutiCar getMarutiCar(String color, double price, String model) {
		return new MarutiCar(color, price, model);
	}
	
	public PulsarBike getPulsarBike(String color, double price, String model) {
		return new PulsarBike(color, price, model);
	}
}
